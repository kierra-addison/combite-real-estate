<div class="row">
  <div class="col-md-6 col-md-offset-3">
    <%= render 'shared/errors', obj: @post_attribute %>

    <div class="panel panel-default">
      <div class="panel-body">
        <h1>Editing Post Attribute</h1>

        <%= render 'form', post_attribute: @post_attribute, action: 'update' %>

        <%= link_to 'Back', edit_post_url(id: @post_attribute.post_id) %>

      </div>
    </div>
  </div>
</div>
