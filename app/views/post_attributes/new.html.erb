<div class="row">
  <div class="col-md-6 col-md-offset-3">
    <%= render 'shared/errors', obj: @post_attribute %>
    <%= render 'shared/flash' if flash_list %>

    <div class="panel panel-default">
      <div class="panel-heading">
        <h1>New Post Attribute</h1>
      </div>
      <div class="panel-body">
        <%= render 'form', post_attribute: @post_attribute, action: 'create' %>
      </div>
    </div>
  </div>
</div>
